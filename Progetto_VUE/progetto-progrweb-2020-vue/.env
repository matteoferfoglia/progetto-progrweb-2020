# root dell'applicazione Vue
BASE_URL=/

VUE_APP_NOME_APPLICAZIONE=FileSharing
VUE_APP_PERCORSO_LOGO=${BASE_URL}favicon.ico

# Costanti
    # RegEx codice fiscale tratto da https://regexlib.com/(A(CAg_Bth78XI7Ry2C7vo_2HR3yuG9GuP1YeCHLd1AH53pIpI-z7JHENTvKnhDjJJhOkjyka4kah-CTZaupEY3MAVWa6qYC256houEBaNXoG01))/UserPatterns.aspx?authorId=10d43491-1297-481f-ae66-db9f2263575c&AspxAutoDetectCookieSupport=1
    VUE_APP_REGEX_CODICE_FISCALE=^([A-Za-z]{6}[0-9lmnpqrstuvLMNPQRSTUV]{2}[abcdehlmprstABCDEHLMPRST]{1}[0-9lmnpqrstuvLMNPQRSTUV]{2}[A-Za-z]{1}[0-9lmnpqrstuvLMNPQRSTUV]{3}[A-Za-z]{1})|([0-9]{11})$
    # RegEx mail tratto da: https://html.spec.whatwg.org/multipage/input.html
    VUE_APP_REGEX_MAIL=^[a-zA-Z0-9.!#$%&'*+\\/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$
    # TODO: verificare correttezza RegEx del codice fiscale e della mail, tratto da:


# Parametri del form di login
    VUE_APP_LOGIN_SERVER_URL=${VUE_APP_SERVER_BASE_URL}api/login

    # nome del parametro contenente username/password inviato dal form
    VUE_APP_LOGIN_USERNAME_INPUT_FIELD_NAME=username
    VUE_APP_LOGIN_PASSWORD_INPUT_FIELD_NAME=password

# Parametri del form di registrazione
    VUE_APP_SIGNUP_CONSUMER_SERVER_URL=${VUE_APP_SERVER_BASE_URL}api/registrazioneNuovoConsumer
    VUE_APP_SIGNUP_CONSUMER_CODFISC_INPUT_FIELD_NAME=codiceFiscale
    VUE_APP_SIGNUP_CONSUMER_NOMECOGNOME_INPUT_FIELD_NAME=nomeCognome
    VUE_APP_SIGNUP_CONSUMER_EMAIL_INPUT_FIELD_NAME=email
    VUE_APP_SIGNUP_CONSUMER_PASSWORD_INPUT_FIELD_NAME=password