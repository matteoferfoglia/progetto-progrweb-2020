# root dell'applicazione Vue
BASE_URL=/

# TODO : verificare che tutte le variabili d'ambiente sono state usate e se ha senso rimuoverle

VUE_APP_TIPO_UTENTE_AUTENTICATO_CONSUMER=Consumer
VUE_APP_TIPO_UTENTE_AUTENTICATO_UPLOADER=Uploader
VUE_APP_TIPO_UTENTE_AUTENTICATO_ADMINISTRATOR=Administrator

# URL server
    VUE_APP_NOME_APPLICAZIONE=FileSharing
    VUE_APP_PERCORSO_LOGO=${VUE_APP_SERVER_BASE_URL}favicon.ico
    VUE_APP_SERVER_BASE_URL_API=${VUE_APP_SERVER_BASE_URL}api/
    VUE_APP_LOGIN_SERVER_URL=${VUE_APP_SERVER_BASE_URL_API}login
    VUE_APP_LOGOUT_SERVER_URL=${VUE_APP_SERVER_BASE_URL_API}logout
    VUE_APP_REGISTRAZIONE_CONSUMER_SERVER_URL=${VUE_APP_SERVER_BASE_URL_API}registrazioneNuovoConsumer
    VUE_APP_GET_CSRF_TOKEN_URL=${VUE_APP_SERVER_BASE_URL_API}CSRFToken/generaCSRFToken
    VUE_APP_VERIFICA_TOKEN_AUTENTICAZIONE=${VUE_APP_SERVER_BASE_URL_API}verificaTokenAutenticazione
    VUE_APP_GET_TIPO_UTENTE_AUTENTICATO=${VUE_APP_SERVER_BASE_URL_API}getTipoUtenteAutenticato

    VUE_APP_GET_NOME_QUESTO_ATTORE_AUTENTICATO = ${VUE_APP_SERVER_BASE_URL_API}nomeDiQuestoAttore

    # Richieste dei Consumer
        VUE_APP_URL_RICHIESTE_CONSUMER=${VUE_APP_SERVER_BASE_URL_API}consumer/

        # RichiestE info su un uploader (appendere in coda l'id dell'Uploader come @PathParam, es.: .../{012idUploader345} ):
        VUE_APP_GET_INFO_UPLOADER=${VUE_APP_URL_RICHIESTE_CONSUMER}proprietaUploader
        VUE_APP_GET_LOGO_UPLOADER=${VUE_APP_URL_RICHIESTE_CONSUMER}logoUploader

        # Richiesta nomi delle properties restituite nelle info di un Uploader
        VUE_APP_GET_NOME_PROP_NOME_UPLOADER=${VUE_APP_URL_RICHIESTE_CONSUMER}nomeProprietaNomeUploader
        VUE_APP_GET_NOME_PROP_LOGO_UPLOADER=${VUE_APP_URL_RICHIESTE_CONSUMER}nomeProprietaLogoUploader

    # Richiesta elenco Uploader
        # Richiesta mappa { idUploader => [listaIdFileCaricatiPerQuestoConsumerDaUploaderInChiave] }
        VUE_APP_GET_ID_UPLOADER_FILE_PER_QUESTO_CONSUMER=${VUE_APP_URL_RICHIESTE_CONSUMER}mappaId-uploader-files

    # Richiesta documenti # TODO : rivedere questi url assegnati (magari trovarne di più significativi)
        VUE_APP_URL_DOCUMENTI_CONSUMER=${VUE_APP_URL_RICHIESTE_CONSUMER}documenti/
        VUE_APP_GET_INTESTAZIONE_TABELLA_DOCUMENTI=${VUE_APP_URL_DOCUMENTI_CONSUMER}arrayNomiProprietaOgniDocumento
        # url per il download di un documeto (appendere in coda l'id del documento da scaricare come @PathParam, es.: .../{012idDocumento345} ):
        VUE_APP_URL_DOWNLOAD_DOCUMENTO=${VUE_APP_URL_DOCUMENTI_CONSUMER}downloadDocumento

        # Richiesta documenti da un uploader (appendere in coda l'id dell'Uploader come @PathParam, es.: .../{012idUploader345} ):
        VUE_APP_GET_DOCUMENTI_CONSUMER=${VUE_APP_URL_DOCUMENTI_CONSUMER}elencoDocumenti
        VUE_APP_GET_DOCUMENTI_CONSUMER_NOME_PROP_HAHSTAGS=${VUE_APP_URL_DOCUMENTI_CONSUMER}nomePropHashtags
        VUE_APP_GET_DOCUMENTI_CONSUMER_NOME_PROP_DATA_CARICAMENTO=${VUE_APP_URL_DOCUMENTI_CONSUMER}nomePropDataCaricamento
        VUE_APP_GET_DOCUMENTI_CONSUMER_NOME_PROP_DATA_VISUALIZZAZIONE=${VUE_APP_URL_DOCUMENTI_CONSUMER}nomePropDataVisualizzazione


    # Richieste degli Uploader
        VUE_APP_URL_RICHIESTE_UPLOADER=${VUE_APP_SERVER_BASE_URL_API}uploader/

        # Richiesta mappa idConsumer-listaIdFileCaricatiPerConsumerDa { idConsumer => [listaIdFileCaricatiDaQuestoUploaderPerConsumerInChiave] }
        VUE_APP_GET_ID_CONSUMER_FILE_PER_QUESTO_CONSUMER=${VUE_APP_URL_RICHIESTE_UPLOADER}mappaId-consumer-files

        # Richiesta info su un Consumer (appendere in coda l'id del Consumer come @PathParam, es.: .../{012idConsumer345} ):
        VUE_APP_GET_INFO_CONSUMER=${VUE_APP_URL_RICHIESTE_UPLOADER}proprietaConsumer

        # Richiesta lista degli id dei consumer aventi username iniziante con quello specificato nel @PathParam
        VUE_APP_GET_ID_CONSUMER_INIZIANTE_CON=${VUE_APP_URL_RICHIESTE_UPLOADER}ricercaConsumer

        # Richiesta dei nomi delle proprietà dell'oggetto restituito al server contenente le info di un consumer come property
        VUE_APP_GET_NOME_PROP_NOME_CONSUMER=${VUE_APP_URL_RICHIESTE_UPLOADER}nomeProprietaNomeConsumer
        VUE_APP_GET_NOME_PROP_EMAIL_CONSUMER=${VUE_APP_URL_RICHIESTE_UPLOADER}nomeProprietaEmailConsumer
        VUE_APP_GET_NOME_PROP_USERNAME_CONSUMER=${VUE_APP_URL_RICHIESTE_UPLOADER}nomeProprietaUsernameConsumer

        # Elimina consumer dalla lista di quelli gestiti dall'Uploader che ne fa richiesta (appendere in coda l'id del Consumer da eliminare come @PathParam, es.: .../{012idConsumer345} ):
        VUE_APP_DELETE_CONSUMER_PER_QUESTO_UPLOADER=${VUE_APP_URL_RICHIESTE_UPLOADER}cancellaConsumerPerQuestoUploader

        # Aggiungi consumer alla lista di quelli gestiti dall'Uploader che ne fa richiesta ( attende le properties di un Consumer tramite POST ):
        VUE_APP_ADD_CONSUMER_PER_QUESTO_UPLOADER=${VUE_APP_URL_RICHIESTE_UPLOADER}aggiungiConsumerPerQuestoUploader


# Autenticazione: nome della variabile del local storage del browser in cui salvare il token di autenticazione
    VUE_APP_NOME_VARIABILE_IN_LOCAL_STORAGE_CON_TOKEN_AUTENTICAZIONE=tokenAutenticazione

# Parametri di configurazione Vue-Router (url-path, nomi route, )
    VUE_APP_ROUTER_ROOT_PATH=/
    VUE_APP_ROUTER_AUTENTICAZIONE_PATH=/autenticazione
    VUE_APP_ROUTER_PATH_LOGIN=${VUE_APP_ROUTER_AUTENTICAZIONE_PATH}/login
    VUE_APP_ROUTER_NOME_ROUTE_LOGIN=Login
    VUE_APP_ROUTER_PATH_REGISTRAZIONE_CONSUMER=${VUE_APP_ROUTER_AUTENTICAZIONE_PATH}/registrazione-consumer
    VUE_APP_ROUTER_PATH_AREA_RISERVATA=/area-riservata
    VUE_APP_ROUTER_NOME_COMPONENTE_AREA_RISERVATA=AreaRiservata
    VUE_APP_ROUTER_PATH_LISTA_DOCUMENTI=/listaDocumenti
    VUE_APP_ROUTER_NOME_LISTA_DOCUMENTI=ListaDocumenti
    VUE_APP_ROUTER_PATH_LISTA_DOCUMENTI_SCARICATI_DA_CONSUMER=/listaDocumentiScaricatiDaConsumer
    VUE_APP_ROUTER_NOME_LISTA_DOCUMENTI_SCARICATI_DA_CONSUMER=ListaDocumentiScaricatiDaConsumer
    # Di seguito, nomi dei parametri usati in Vue Router (ad es. il token di autenticazione del client)
    VUE_APP_ROUTER_PARAMETRO_TOKEN_AUTENTICAZIONE=${VUE_APP_NOME_VARIABILE_IN_LOCAL_STORAGE_CON_TOKEN_AUTENTICAZIONE}
    VUE_APP_ROUTER_PARAMETRO_ID_UPLOADER_DI_CUI_MOSTRARE_DOCUMENTI_PER_CONSUMER=idUploader
    VUE_APP_ROUTER_PARAMETRO_LOGO_UPLOADER_DI_CUI_MOSTRARE_DOCUMENTI_PER_CONSUMER=logoUploader
    # Nel caso di login dovuti a richieste a pagine non autorizzate può essere comodo salvarsi tali pagine per poter fare redirection se il login va a buon fine:
    VUE_APP_ROUTER_PARAMETRO_FULLPATH_ROUTE_RICHIESTA_PRIMA=fullpathRoutePrecedente
    VUE_APP_ROUTER_PARAMETRO_PARAMS_ROUTE_RICHIESTA_PRIMA=paramsRoutePrecedente

# Costanti
    # RegEx codice fiscale tratto da https://regexlib.com/(A(CAg_Bth78XI7Ry2C7vo_2HR3yuG9GuP1YeCHLd1AH53pIpI-z7JHENTvKnhDjJJhOkjyka4kah-CTZaupEY3MAVWa6qYC256houEBaNXoG01))/UserPatterns.aspx?authorId=10d43491-1297-481f-ae66-db9f2263575c&AspxAutoDetectCookieSupport=1
    VUE_APP_REGEX_CODICE_FISCALE=^([A-Za-z]{6}[0-9lmnpqrstuvLMNPQRSTUV]{2}[abcdehlmprstABCDEHLMPRST]{1}[0-9lmnpqrstuvLMNPQRSTUV]{2}[A-Za-z]{1}[0-9lmnpqrstuvLMNPQRSTUV]{3}[A-Za-z]{1})|([0-9]{11})$
    # RegEx mail tratto da: https://html.spec.whatwg.org/multipage/input.html
    VUE_APP_REGEX_EMAIL=^[a-zA-Z0-9.!#$%&'*+\\/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$
    # TODO: verificare correttezza RegEx del codice fiscale e della mail

# CSRF: nome dell'input field contenente il token CSRF
    VUE_APP_CSRF_INPUT_FIELD_NAME=csrfToken

# Parametri del form di autenticazione
    # nome del parametro contenente username/password inviato dal form
    VUE_APP_LOGIN_USERNAME_INPUT_FIELD_NAME=username
    VUE_APP_LOGIN_PASSWORD_INPUT_FIELD_NAME=password

# Parametri del form di registrazione
    VUE_APP_REGISTRAZIONE_CONSUMER_CODFISC_INPUT_FIELD_NAME=codiceFiscale
    VUE_APP_REGISTRAZIONE_CONSUMER_NOMECOGNOME_INPUT_FIELD_NAME=nomeCognome
    VUE_APP_REGISTRAZIONE_CONSUMER_EMAIL_INPUT_FIELD_NAME=email
    VUE_APP_REGISTRAZIONE_CONSUMER_PASSWORD_INPUT_FIELD_NAME=password